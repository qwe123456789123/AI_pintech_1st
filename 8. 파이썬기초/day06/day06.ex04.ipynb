   "source": [
    "# 이터레이터를 만들려면 iter(...)\n",
    "fruits = ['사과','오렌지','망고','멜론']\n",
    "itr = iter(fruits) # 이터레이터 객체"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "852b28e4-ed9c-499e-81a4-6bc674bdcc43",
   "metadata": {},
   "outputs": [],
   "source": [
    "while True:\n",
    "    try:\n",
    "        print(next(itr))\n",
    "    except StopIteration:\n",
    "        break"
   ]
  },
